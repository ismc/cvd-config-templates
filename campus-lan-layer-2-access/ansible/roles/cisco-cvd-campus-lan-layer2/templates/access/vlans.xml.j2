<config>
  <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
      <vlan>

        <vlan-list xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-vlan">
          <id>{{ cvd_data_vlan_id }}</id>
          <name>{{ cvd_data_vlan_name }}</name>
        </vlan-list>

        <vlan-list xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-vlan">
          <id>{{ cvd_voice_vlan_id }}</id>
          <name>{{ cvd_voice_vlan_name }}</name>
        </vlan-list>

        {% if cvd_management_vlan_id %}
        <vlan-list xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-vlan">
          <id>{{ cvd_management_vlan_id }}</id>
          <name>{{ cvd_management_vlan_name }}</name>
        </vlan-list>
        {% endif %}

        {% if cvd_anti_vlan_hop_id %}
        <vlan-list xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-vlan">
          <id>{{ cvd_anti_vlan_hop_id }}</id>
          <name>{{ cvd_anti_vlan_hop_name }}</name>
        </vlan-list>
        {% endif %}

      </vlan>
  </native>
</config>
